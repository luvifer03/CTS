import java.util.*;

public class Main {
	public static void main(String[] args) {
		int a,b,c;
		Scanner sc= new Scanner(System.in);
		System.out.println("ENter first number");
		a= sc.nextInt();
		System.out.println("Enter second number");
		b=sc.nextInt();
		c=a+b;
		System.out.println("Sum: " +c);
		
		
	}

}
